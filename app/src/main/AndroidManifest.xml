<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.patient.patienthelper" >

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PatientHelper"
        tools:targetApi="31" >

        <activity
            android:name=".activitys.community"
            android:exported="false" />
        <activity
            android:name=".activitys.UserProfileActivity"
            android:exported="false" />
        <activity
            android:name=".activitys.VerificationCodeActivity"
            android:exported="false" />
        <activity
            android:name=".activitys.RestPasswordActivity"
            android:exported="false" />
        <activity
            android:name=".activitys.Select_illActivity"
            android:exported="false" />
        <activity
            android:name=".activitys.Splash"
            android:exported="false" />
        <activity
            android:name=".activitys.SignUpActivity"
            android:exported="false" />
        <activity
            android:name=".activitys.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".activitys.LookingForActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activitys.MainActivity"
            android:exported="false" >

        </activity>
    </application>

</manifest>